generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "database.sqlite"
}

model Password {
    id String @id @default(uuid())
    name String
    password String
    createdAt DateTime @default(now())    
    updatedAt DateTime

    @@index([name])
}

model Config {
    id Int @id
    firstEntry Boolean @default(true)
}