generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./database.sqlite"
}

model Password {
    id String @id @default(uuid())
    name String @unique
    password String 
    createdAt DateTime @default(now())    
    updatedAt DateTime @updatedAt

    @@index([name])
}

model Config {
    id Int @id
    firstEntry Boolean @default(true)
    systemPassword String
}